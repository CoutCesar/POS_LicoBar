delete from Producto;
delete from Ventas;
DELETE FROM Venta_Producto;


select * from Producto;
select * from Ventas;
select * from Venta_Producto;

update Producto
set Existencias = 0;




UPDATE Ventas
SET Costo_Venta = 0.0, Ingresos_Venta = 0.0, Utilidad = 0.0;

ALTER TABLE Producto
ADD COLUMN Existencias INT;



